{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["eslint:recommended","plugin:prettier/recommended","plugin:security/recommended", "plugin:node/recommended"],
    "plugins": ["prettier","security", "node"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "prettier/prettier": "error"
    }
}
